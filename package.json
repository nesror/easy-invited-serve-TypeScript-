{
  "name": "easy-invited",
  "version": "1.0.0",
  "description": "easy-invited",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --port=3000 --daemon --title=egg-server-wtsp",
    "stop": "egg-scripts stop --title=egg-server-wtsp",
    "dev": "egg-bin dev --port=3000",
    "debug": "egg-bin debug --port=3000",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/validator": "^12.0.1",
    "egg": "^2.15.1",
    "egg-alinode": "^2.0.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-view-nunjucks": "^2.2.0",
    "formstream": "^1.1.0",
    "koa-body": "^4.1.1",
    "mysql2": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "sqlite3": "^4.1.0",
    "stream-wormhole": "^1.1.0",
    "wechat-encrypt": "1.1.0",
    "wormhole": "^3.0.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.6",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.10.9",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "nestor",
  "license": "MIT"
}
